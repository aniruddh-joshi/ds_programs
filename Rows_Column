  #include<stdio.h>
  int main()
  {
  	int i,j,r,c;
  	printf("Enter the number of rows\n");
  	scanf("%d",&r);
  	printf("Enter the number of column\n");
  	scanf("%d",&c);
  	
  	for(i=r-1;i>=0;i--)
  	{
  		for(j=c-1;j>=0;j--)
  		{
  			printf("%d",i+1);
  			printf("\t");
		}
		printf("\n");
	}
	return 0;
  }
